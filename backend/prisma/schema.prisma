datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model users {
  id           Int     @id @default(autoincrement())
  name         String
  email        String  @unique
  passwordHash String
}
model posts {
  id          String   @id @default(cuid())
  title       String
  content     String
  created_at  DateTime @default(now())
  user_id     Int
}
generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}